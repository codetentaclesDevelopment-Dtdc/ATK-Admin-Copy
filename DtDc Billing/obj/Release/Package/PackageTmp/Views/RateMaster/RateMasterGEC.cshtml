<script>
    $(document).ready(function () {

        //$('#DoxTable td:nth-child(2)').hide();
        //$('#DoxTable td:nth-child(3)').hide();

        //$('#DoxTable th:nth-child(2)').hide();
        //$('#DoxTable th:nth-child(3)').hide();

        var data = '@ViewBag.GECratemaster';
        var test = 2;
        if (data != 0 && data!=null) {

                    var test = '@ViewBag.GECratemaster[0].NoOfSlab';

        }

        //alert(test);

        console.log(test);

        if (test == 2) {
            $('.slab2').hide();
            $('.slab3').hide();

            $('#only').val(2);

        }
        else if (test == 3) {


            $('.slab2').show();




            $('.slab3').hide();


            $('#only').val(test);
        }
        else if (test == 4) {

            $('.slab2').show();
            $('.slab3').show();



            $('#only').val(test);
        }



        $("input[name$='NoOfSlab']").change(function () {

            var test = $(this).val();

            //alert(test);

            console.log(test);

            if (test == 2) {
                $('.slab2').hide();
                $('.slab3').hide();

                $('#only').val(2);

            }

            else if (test == 3) {


                $('.slab2').show();




                $('.slab3').hide();


                $('#only').val(test);
            }
            else if (test == 4) {

                $('.slab2').show();
                $('.slab3').show();



                $('#only').val(test);
            }


        });
    });
</script>







