<script>
    $(document).ready(function () {

        var getValue = '@ViewBag.AllProduct';
        //alert(getValue);

        if (getValue == 0) {

            $('#frmEmp1').show();
            $('#frmEmp2').show();
            $('#frmEmp3').show();
            $('#frmEmp4').show();
            $('#frmEmp5').show();
            $('#frmEmp6').show();
            $('#frmEmp7').show();
            $('#frmEmp8').show();
            $('#frmEmp10').show();


            $('#doxheader').show();
            $('#Ndoxheader').show();
            $('#plusheader').show();
            $('#ptpheader').show();
            $('#expheader').show();
            $('#priheader').show()
            $('#lapheader').show();
            $('#Revlapheader').show();
            $('#Ecomheader').show();

        }

        else
        { 
        if (getValue.includes("Ratem"))
        {
            $('#frmEmp1').show();
            $('#doxheader').show();
        }
        else
        {
            $('#frmEmp1').hide();
            $('#doxheader').hide();
           
        }
        if (getValue.includes("NonDox"))
        {
            $('#frmEmp2').show();
            $('#Ndoxheader').show();
        }
        else
        {
            $('#frmEmp2').hide();
            $('#Ndoxheader').hide();
        }

        if (getValue.includes("DTDCPLus"))
        {
            $('#frmEmp3').show();
            $('#plusheader').show();
            
        }
        else
        {
            $('#frmEmp3').hide();
            $('#plusheader').hide();
        }

        if (getValue.includes("DTDCPTP")) {
            $('#frmEmp4').show();
            $('#ptpheader').show();
            
        }
        else {
            $('#frmEmp4').hide();
            $('#ptpheader').hide();
        }
        if (getValue.includes("ExpressCargo")) {
            $('#frmEmp5').show();
            $('#expheader').show();
            
        }
        else {
            $('#frmEmp5').hide();
            $('#expheader').hide();
        }
        if (getValue.includes("Priority")) {
            $('#frmEmp6').show();
            $('#priheader').show();
            
        }
        else {
            $('#frmEmp6').hide();
            $('#priheader').hide();
        }
        if (getValue.includes("Laptops")) {
            $('#frmEmp7').show();
            $('#lapheader').show();
        }
        else {
            $('#frmEmp7').hide();
            $('#lapheader').hide();
        }
      
        if (getValue.includes("RevPickupLaptops")) {
            $('#frmEmp8').show();
            $('#Revlapheader').show();
            
        }
        else {
            $('#frmEmp8').hide();
            $('#Revlapheader').hide();
        }
        if (getValue.includes("Ecommerce")) {
            $('#frmEmp10').show();
            $('#Ecomheader').show();
            
        }
        else {
            $('#frmEmp10').hide();
            $('#Ecomheader').hide();
            }
            if (getValue.includes("GECMode")) {
                $("#frmEmp11").show();
                $("#GECheader").show();
            } else {
                $("#frmEmp11").hide();
                $("#GECheader").hide();
            }
     }
       
            });
</script>









