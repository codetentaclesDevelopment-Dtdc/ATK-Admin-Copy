
@{
    ViewBag.Title = "Index";
    Html.EnableClientValidation(true);
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}





<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-info">


                <div class="box-body">

                    <ul id="Tab" class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="#home">Add Company</a></li>
                        <li><a data-toggle="tab" href="#menu1" id="doxheader">Dox</a></li>
                        <li><a data-toggle="tab" href="#menu2" id="Ndoxheader">NonDox</a></li>
                        <li><a data-toggle="tab" href="#menu3" id="plusheader">Dtdc PLUS</a></li>
                        <li><a data-toggle="tab" href="#menu4" id="ptpheader">Dtdc PTP</a></li>
                        <li><a data-toggle="tab" href="#menu5" id="expheader">Express Cargo</a></li>
                        <li><a data-toggle="tab" href="#menu6" id="priheader">Priority</a></li>
                        <li><a data-toggle="tab" href="#menu7" id="lapheader">Laptops</a></li>
                        <li><a data-toggle="tab" href="#menu8" id="Revlapheader">Reverse Pickup Laptops</a></li>
                        <li><a data-toggle="tab" href="#menu9">Upload Documents</a></li>
                        <li><a data-toggle="tab" href="#menu10" id="Ecomheader">Ecommerce</a></li>
                        <li><a data-toggle="tab" href="#menu11" id="GECheader">GEC</a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="home" class="tab-pane fade in active">
                            <div id="frmEmp">
                                @{ Html.RenderPartial("RateMasterEditCompany", (DtDc_Billing.Entity_FR.Company)ViewBag.Company);}
                            </div>
                        </div>

                        <div id="menu1" class="tab-pane fade">
                            <div id="frmEmp1">
                                @{ Html.RenderPartial("RateMasterDox", (IEnumerable<DtDc_Billing.Entity_FR.Ratem>)ViewBag.Dox);}
                            </div>
                        </div>


                        <div id="menu2" class="tab-pane fade">
                            <div id="frmEmp2">
                                @{ Html.RenderPartial("RateMasterNonDox", (IEnumerable<DtDc_Billing.Entity_FR.Nondox>)ViewBag.NonDox);}
                            </div>
                        </div>
                        <div id="menu3" class="tab-pane fade">
                            <div id="frmEmp3">
                                @{ Html.RenderPartial("RateMasterPlus", (IEnumerable<DtDc_Billing.Entity_FR.dtdcPlu>)ViewBag.Plus);}
                            </div>
                        </div>
                        <div id="menu4" class="tab-pane fade">
                            <div id="frmEmp4">
                                @{ Html.RenderPartial("RateMasterPtp", (IEnumerable<DtDc_Billing.Entity_FR.Dtdc_Ptp>)ViewBag.Ptp);}
                            </div>
                        </div>
                        <div id="menu5" class="tab-pane fade">
                            <div id="frmEmp5">
                                @{ Html.RenderPartial("RateMasterExpressCargo", (IEnumerable<DtDc_Billing.Entity_FR.express_cargo>)ViewBag.Cargo);}
                            </div>
                        </div>
                        <div id="menu6" class="tab-pane fade">
                            <div id="frmEmp6">
                                @{ Html.RenderPartial("Priority", (IEnumerable<DtDc_Billing.Entity_FR.Priority>)ViewBag.Priority);}
                            </div>
                        </div>
                        <div id="menu7" class="tab-pane fade">
                            <div id="frmEmp7">
                                @{ Html.RenderPartial("RateMasterLaptops", (IEnumerable<DtDc_Billing.Entity_FR.RateLaptop>)ViewBag.Laptops);}
                            </div>
                        </div>
                        <div id="menu8" class="tab-pane fade">
                            <div id="frmEmp8">
                                @{ Html.RenderPartial("RateMasterRevLaptops", (IEnumerable<DtDc_Billing.Entity_FR.RateRevLaptop>)ViewBag.RevLaptops);}
                            </div>
                        </div>
                        <div id="menu9" class="tab-pane fade">
                            <div id="frmEmp9">
                                @{ Html.RenderPartial("UploadDocument", (DtDc_Billing.Models.AddlogoModel)ViewBag.Uploaddoc);}
                            </div>
                        </div>
                        <div id="menu10" class="tab-pane fade">
                            <div id="frmEmp10">
                                @{ Html.RenderPartial("RateMasterNewEcommerce", (IEnumerable<DtDc_Billing.Entity_FR.NewDtdc_Ecommerce>)ViewBag.Ecommerce);}
                            </div>
                        </div>
                        <div id="menu11" class="tab-pane fade">
                            <div id="frmEmp11">

                                <!--Pending Work So Invisible these Property-->
                                @{ Html.RenderPartial("RateMasterGEC", (IEnumerable<DtDc_Billing.Models.GECrateModel>)ViewBag.GECratemaster);}
                            </div>
                        </div>

                        <input type="hidden" id="tabbb" name="tab" />
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
