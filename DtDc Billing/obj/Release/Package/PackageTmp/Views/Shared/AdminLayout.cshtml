    <script>
        $(document).ready(function () {
            // Fetch pending requests on page load
            $(".close").click(function () {
                $("#requestUpdateModal").modal('hide');
            })
            $.ajax({
                url: '/CashBooking/GetReuestToUpdateAmount', // Replace with your controller name
                type: 'GET',
                success: function (data) {
                    console.log(data); // For debugging purposes
                    if (data.length > 0) {
                        // Inject data into the modal table
                        let tableBody = $("#requestUpdateTableBody");
                        tableBody.empty(); // Clear existing rows

                        data.forEach((item, index) => { 
                            let row = `
                            <tr>
                              
                                <td>${item.Consignment_No || "N/A"}</td>
                                <td>${item.Charges_Total || "0"}</td>
                                <td>${item.UpdatedAmount || "0"}</td>
                                <td>${item.Remark || "No Remark"}</td>
                                <td>${item.Pf_Code || "N/A"}</td>
                                <td>${item.EmployeeName || "Unknown"}</td>
                                <td>
                                    <button class="btn btn-success btn-sm" onclick="approveRequest(${item.Receipt_Id})">Approve</button>
                                    <button class="btn btn-danger btn-sm" onclick="rejectRequest(${item.Receipt_Id})">Reject</button>
                                </td>
                            </tr>`;
                            tableBody.append(row);
                        });

                        // Show the modal
                        $("#requestUpdateModal").modal('show');
                    } else {
                        alert("No pending requests found.");
                    }
                },
                error: function (xhr, status, error) {
                    console.error("Error fetching update requests:", error);
                }
            });
        });

        // Approve Request
        function approveRequest(id) {
            $.ajax({
                url: `/CashBooking/ApproveRequest/${id}`, // Replace with your controller/action
                type: 'POST',
                success: function (response) {
                    alert(response.message);
                    location.reload(); // Reload the page to refresh data
                },
                error: function (xhr, status, error) {
                    alert("Error processing the request.");
                }
            });
        }

        // Reject Request
        function rejectRequest(id) {
            $.ajax({
                url: `/CashBooking/RejectRequest/${id}`, // Replace with your controller/action
                type: 'POST',
                success: function (response) {
                    alert(response.message);
                    location.reload(); // Reload the page to refresh data
                },
                error: function (xhr, status, error) {
                    alert("Error processing the request.");
                }
            });
        }
        setInterval(function () {
            $.ajax({
                url: '/CashBooking/GetReuestToUpdateAmount', // Replace with your controller name
                type: 'GET',
                success: function (data) {
                    console.log(data); // For debugging purposes
                    if (data.length > 0) {
                        // Inject data into the modal table
                        let tableBody = $("#requestUpdateTableBody");
                        tableBody.empty(); // Clear existing rows

                        data.forEach((item, index) => {
                            let row = `
                  <tr>
                    
                      <td>${item.Consignment_No || "N/A"}</td>
                      <td>${item.Charges_Total || "0"}</td>
                      <td>${item.UpdatedAmount || "0"}</td>
                      <td>${item.Remark || "No Remark"}</td>
                      <td>${item.Pf_Code || "N/A"}</td>
                      <td>${item.EmployeeName || "Unknown"}</td>
                      <td>
                          <button class="btn btn-success btn-sm" onclick="approveRequest(${item.Receipt_Id})">Approve</button>
                          <button class="btn btn-danger btn-sm" onclick="rejectRequest(${item.Receipt_Id})">Reject</button>
                      </td>
                  </tr>`;
                            tableBody.append(row);
                        });

                        // Show the modal
                        $("#requestUpdateModal").modal('show');
                    } 
                },
                error: function (xhr, status, error) {
                    console.error("Error fetching update requests:", error);
                }
            });
        }, 300000); // Check every 5 minutes

    </script>

