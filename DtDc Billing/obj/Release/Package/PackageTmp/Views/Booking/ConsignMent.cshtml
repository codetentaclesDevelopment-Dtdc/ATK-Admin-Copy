@model DtDc_Billing.Metadata_Classes.TransactionMetadata


@{
    
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-info">
                @using (Html.BeginForm("importTextFile", "Admin", FormMethod.Post, new { enctype = "multipart/form-data", id = "importtext" }))
    {

            @Html.AntiForgeryToken()
         

                <div class="box-header with-border">
                    <div class="col-sm-7">
                       
                            <h4>
                                Book Consignment
                            </h4>
                       
                        <div class="col-sm-4 col-md-offset-2">
                            <input type="text" id="intpincode" class="form-control" style="display:none" placeholder="International City" />
                        </div>
                    </div>
                    <div class="col-sm-5">
                        <div class="col-sm-8">
                            <input id="File1" required type="file" name="ImportText" />
                        </div>
                        <div class="col-sm-4">
                            <input type="submit" class="btn btn-primary pull-right" value="Import From text" />
                        </div>
                    </div>
                </div>
          
}

                <div class="form-horizontal">
                    <div class="box-body">
                        <div id="form-Cons">
                            @Html.Partial("ConsignmentPartial", Model)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

