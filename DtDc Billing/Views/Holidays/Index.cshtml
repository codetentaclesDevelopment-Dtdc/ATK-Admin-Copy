@model IEnumerable<DtDc_Billing.Entity_FR.Holiday>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.H_Year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.H_Month)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.H_datetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Holiday_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.H_Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.H_Month)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.H_datetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Holiday_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Holiday_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Holiday_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Holiday_ID })
        </td>
    </tr>
}

</table>
