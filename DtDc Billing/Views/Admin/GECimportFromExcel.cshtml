
@{

    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<section class="content">
    <div class="row">
        <div class="col-sm-12">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h4>IMPORT FROM EXCEL</h4>
                </div>

                <div class="form-horizontal">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    @using (Html.BeginForm("GECimportFromExcel", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                    {
                                        <div class="col-md-3">
                                            <label for="file"><strong id="up-txt"> Upload Excel </strong></label>
                                        </div>
                                        <div class="col-md-6">
                                            <input type="file" name="httpPostedFileBase" id="file" class="form-control" />
                                        </div>
                                        <div class="col-md-1">
                                            <input type="submit" value="Upload" class="btn btn-primary" />
                                        </div>
                                      
                                    }
                                </div>
                            </div>
                        </div>
                        <hr />

                    </div>
                </div>

            </div>
        </div>
    </div>
</section>


