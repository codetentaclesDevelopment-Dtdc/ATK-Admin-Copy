        <th>
            @Html.DisplayNameFor(model => model.inserteddate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tempinserteddate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Invoiceno)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.inserteddate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tempinserteddate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Invoiceno)
        </td>
